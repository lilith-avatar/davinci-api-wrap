{
	"CMod":{
		"prefix" : "CMod", //可以改成你想要的单词
		"body": [
			"---@module ${1:testModule}",
			"---@copyright Lilith Games, Avatar Team",
			"---@author XXX, XXXX",
			"local ${1:testModule},this = {},nil",
			"\n---初始化函数",
			"function ${1:testModule}:Init()",
			"    this = self",
			"    self:InitListener()"
			"end",
			"\n---监听函数",
			"function ${1:testModule}:InitListener()",
			"    EventUtil.LinkConnects(localPlayer.C_Event, ${1:testModule}, '${1:testModule}', this)",
			"end",
			"\n---Update函数",
			"function ${1:testModule}:Update()",
			"end",
			"\nreturn ${1:testModule}"
		],
		"description": "创建客户端模块"
	},
